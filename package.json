{
  "name": "teste-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "check:type": "tsc --noEmit",
    "check:type:dev": "tsc --noEmit --watch",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint --fix --ext .ts,.tsx,.json src/",
    "test": "vitest --run --threads",
    "test:dev": "vitest",
    "ci:lint": "eslint --max-warnings 0 --ext .ts,.tsx,.json src/",
    "fake:api": "npx json-server --watch db.json --port 3001"
  },
  "dependencies": {
