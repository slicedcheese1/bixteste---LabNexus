    "@mui/material": "^5.15.11",
    "@mui/x-data-grid": "^6.19.6",
    "@mui/x-date-pickers": "^7.3.2",
    "@tanstack/react-query": "^5.12.2",
    "axios": "^1.4.0",
    "buffer": "^6.0.3",
    "date-fns": "^3.6.0",
    "jsonwebtoken": "^8.5.1",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-router-dom": "^6.18.0",
    "recharts": "^2.12.6",
    "zod": "^3.21.4"
